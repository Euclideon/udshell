<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/epshell">
    <sub-class-of type="text/xml"/>
    <glob pattern="*.epproj"/>
    <generic-icon name="euclideon"/>
    <comment>Euclideon Platform Project File</comment>
    <acronym>EPProj</acronym>
    <expanded-acronym>Euclideon Platform Project</expanded-acronym>
  </mime-type>

  <mime-type type="application/uds">
    <comment>Unlimited Detail Model</comment>
    <generic-icon name="euclideon"/>
    <glob pattern="*.oct3"/>
    <glob pattern="*.udm"/>
    <glob pattern="*.udg"/>
    <glob pattern="*.udi"/>
    <glob pattern="*.uds"/>
    <glob pattern="*.uds3"/>
    <glob pattern="*.uds4"/>
    <glob pattern="*.uds5"/>
    <glob pattern="*.upc"/>
  </mime-type>
</mime-info>

